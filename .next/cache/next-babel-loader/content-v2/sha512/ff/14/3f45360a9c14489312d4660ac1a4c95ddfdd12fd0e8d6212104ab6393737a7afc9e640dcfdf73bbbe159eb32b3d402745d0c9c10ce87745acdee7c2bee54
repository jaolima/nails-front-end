= useContext(WishlistContext);\r\n  const compareContext = useContext(CompareContext);\r\n  const symbol = curContext.state.symbol;\r\n  const currency = curContext.state;\r\n  const cartCtx = useContext(CartContext);\r\n  const addToCart = cartCtx.addToCart;\r\n  const quantity = cartCtx.quantity;\r\n  const plusQty = cartCtx.plusQty;\r\n  const minusQty = cartCtx.minusQty;\r\n  const setQuantity = cartCtx.setQuantity;\r\n  const [selectedProduct, setSelectedProduct] = useState();\r\n  const [modal, setModal] = useState(false);\r\n  const toggle = () => setModal(!modal);\r\n  const uniqueTags = [];\r\n\r\n  const changeQty = (e) => {\r\n    setQuantity(parseInt(e.target.value));\r\n  };\r\n\r\n  const clickProductDetail = (product) => {\r\n    const titleProps = product.title.split(\" \").join(\"\");\r\n    router.push(`/product-details/${product.id}` + \"-\" + `${titleProps}`);\r\n  };\r\n\r\n  const getSelectedProduct = (item) => {\r\n    setSelectedProduct(item);\r\n    toggle();\r\n  };\r\n\r\n  var { loading, data } = useQuery(GET_PRODUCTS, {\r\n    variables: {\r\n      type: \"fashion\",\r\n      indexFrom: 0,\r\n      limit: 8,\r\n    },\r\n  });\r\n\r\n  return (\r\n    <section className=\"section-b-space ratio_asos\">\r\n      <Container>\r\n        <Row>\r\n          <Col className=\"product-related\">\r\n            <h2>related products</h2>\r\n          </Col>\r\n        </Row>\r\n        <Row className=\"search-product\">\r\n          {!data ||\r\n          !data.products ||\r\n          data.products.items.length === 0 ||\r\n          loading ? (\r\n            \"loading\"\r\n          ) : (\r\n            <>\r\n              {data &&\r\n                data.products.items.slice(0, 6).map((product, index) => (\r\n                  <Col xl=\"2\" md=\"4\" sm=\"6\" key={index}>\r\n                    <div className=\"product-box\">\r\n                      <div className=\"img-wrapper\">\r\n                        <div className=\"front\">\r\n                          <a href={null}>\r\n                            <Media\r\n                              onClick={() => clickProductDetail(product)}\r\n                              src={product.images[0].src}\r\n                              className=\"img-fluid blur-up lazyload bg-img\"\r\n                              alt=\"\"\r\n                            />\r\n                          </a>\r\n                        </div>\r\n                        <div className=\"back\">\r\n                          <a href={null}>\r\n                            <Media\r\n                              src={product.images[1].src}\r\n                              className=\"img-fluid blur-up lazyload bg-img\"\r\n                              alt=\"\"\r\n                            />\r\n                          </a>\r\n                        </div>\r\n                        <div className=\"cart-info cart-wrap\">\r\n                          <button\r\n                            data-toggle=\"modal\"\r\n                            data-target=\"#addtocart\"\r\n                            title=\"Add to cart\"\r\n                            onClick={() => addToCart(product, quantity)}\r\n                          >\r\n                            <i className=\"fa fa-shopping-cart\"></i>\r\n                          </button>\r\n                          <a\r\n                            href=\"#\"\r\n                            onClick={() => wishlistContext.addToWish(product)}\r\n                            title=\"Add to Wishlist\"\r\n                          >\r\n                            <i className=\"fa fa-heart\" aria-hidden=\"true\"></i>\r\n                          </a>\r\n                          <a\r\n                            href=\"#\"\r\n                            onClick={() => getSelectedProduct(product)}\r\n                            data-toggle=\"modal\"\r\n                            data-target=\"#quick-view\"\r\n                            title=\"Quick View\"\r\n                          >\r\n                            <i className=\"fa fa-search\" aria-hidden=\"true\"></i>\r\n                          </a>\r\n                          <a\r\n                            href=\"#\"\r\n                            onClick={() => compareContext.addToCompare(product)}\r\n                            title=\"Compare\"\r\n                          >\r\n                            <i className=\"fa fa-refresh\" aria-hidden=\"true\"></i>\r\n                          </a>\r\n                        </div>\r\n                      </div>\r\n                      <div className=\"product-detail\">\r\n                        <div className=\"rating\">\r\n                          <i className=\"fa fa-star\"></i>{\" \"}\r\n                          <i className=\"fa fa-star\"></i>{\" \"}\r\n                          <i className=\"fa fa-star\"></i>{\" \"}\r\n                          <i className=\"fa fa-star\"></i>{\" \"}\r\n                          <i className=\"fa fa-star\"></i>\r\n                        </div>\r\n                        <a href={null}>\r\n                          <h6>{product.title}</h6>\r\n                        </a>\r\n                        <h4>\r\n                          {symbol}\r\n                          {product.price}\r\n                        </h4>\r\n                        <ul className=\"color-variant\">\r\n                          <li className=\"bg-light0\"></li>\r\n                          <li className=\"bg-light1\"></li>\r\n                          <li className=\"bg-light2\"></li>\r\n                        </ul>\r\n                      </div>\r\n                    </div>\r\n                  </Col>\r\n                ))}\r\n            </>\r\n          )}\r\n        </Row>\r\n        {selectedProduct ? (\r\n          <Modal\r\n            isOpen={modal}\r\n            toggle={toggle}\r\n            className=\"modal-lg quickview-modal\"\r\n            centered\r\n          >\r\n            <ModalBody>\r\n              <Row>\r\n                <Col lg=\"6\" xs=\"12\">\r\n                  <div className=\"quick-view-img\">\r\n                    <Media\r\n                      src={`${selectedProduct.images[0].src}`}\r\n                      alt=\"\"\r\n                      className=\"img-fluid\"\r\n                    />\r\n                  </div>\r\n                </Col>\r\n                <Col lg=\"6\" className=\"rtl-text\">\r\n                  <div className=\"product-right\">\r\n                    <h2> {selectedProduct.title} </h2>\r\n                    <h3>\r\n                      {currency.symbol}\r\n                      {(selectedProduct.price * currency.value).toFixed(2)}\r\n                    </h3>\r\n                    {selectedProduct.variants ? (\r\n                      <ul className=\"color-variant\">\r\n                        {uniqueTags ? (\r\n                          <ul className=\"color-variant\">\r\n                            {selectedProduct.type === \"jewellery\" ||\r\n                            selectedProduct.type === \"nursery\" ||\r\n                            selectedProduct.type === \"beauty\" ||\r\n                            selectedProduct.type === \"electronics\" ||\r\n                            selectedProduct.type === \"goggles\" ||\r\n                            selectedProduct.type === \"watch\" ||\r\n                            selectedProduct.type === \"pets\" ? (\r\n                              \"\"\r\n                            ) : (\r\n                              <>\r\n                                {uniqueTags ? (\r\n                                  <ul className=\"color-variant\">\r\n                                    {uniqueTags.map((vari, i) => {\r\n                                      return (\r\n                                        <li\r\n                                          className={vari.color}\r\n                                          key={i}\r\n                                          title={vari.color}\r\n                                          onClick={() =>\r\n                                            variantChangeByColor(\r\n                                              vari.image_id,\r\n                                              selectedProduct.images\r\n                                            )\r\n                                          }\r\n                                        ></li>\r\n                                      );\r\n                                    })}\r\n                                  </ul>\r\n                                ) : (\r\n                                  \"\"\r\n                                )}\r\n                              </>\r\n                            )}\r\n                          </ul>\r\n                        ) : (\r\n                          \"\"\r\n                        )}\r\n                      </ul>\r\n                    ) : (\r\n                      \"\"\r\n                    )}\r\n                    <div className=\"border-product\">\r\n                      <h6 className=\"product-title\">product details</h6>\r\n                      <p>{selectedProduct.description}</p>\r\n                    </div>\r\n                    <div className=\"product-description border-product\">\r\n                      {selectedProduct.size ? (\r\n                        <div className=\"size-box\">\r\n                          <ul>\r\n                            {selectedProduct.size.map((size, i) => {\r\n                              return (\r\n                                <li key={i}>\r\n                                  <a href={null}>{size}</a>\r\n                                </li>\r\n                              );\r\n                            })}\r\n                          </ul>\r\n                        </div>\r\n                      ) : (\r\n                        \"\"\r\n                      )}\r\n                      <h6 className=\"product-title\">quantity</h6>\r\n                      <div className=\"qty-box\">\r\n                        <div className=\"input-group\">\r\n                          <span className=\"input-group-prepend\">\r\n                            <button\r\n                              type=\"button\"\r\n                              className=\"btn quantity-left-minus\"\r\n                              onClick={minusQty}\r\n                              data-type=\"minus\"\r\n                              data-field=\"\"\r\n                            >\r\n                              <i className=\"fa fa-angle-left\"></i>\r\n                            </button>\r\n                          </span>\r\n                          <input\r\n                            type=\"text\"\r\n                            name=\"quantity\"\r\n                            value={quantity}\r\n                            onChange={changeQty}\r\n                            className=\"form-control input-number\"\r\n                          />\r\n                          <span className=\"input-group-prepend\">\r\n                            <button\r\n                              type=\"button\"\r\n                              className=\"btn quantity-right-plus\"\r\n                              onClick={() => plusQty(selectedProduct)}\r\n                              data-type=\"plus\"\r\n                              data-field=\"\"\r\n                            >\r\n                              <i className=\"fa fa-angle-right\"></i>\r\n                            </button>\r\n                          </span>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                    <div className=\"product-buttons\">\r\n                      <button\r\n                        className=\"btn btn-solid\"\r\n                        onClick={() => addToCart(selectedProduct, quantity)}\r\n                      >\r\n                        add to cart\r\n                      </button>\r\n                      <button\r\n                        className=\"btn btn-solid\"\r\n                        onClick={() => clickProductDetail(selectedProduct)}\r\n                      >\r\n                        View detail\r\n                      </button>\r\n                    </div>\r\n                  </div>\r\n                </Col>\r\n              </Row>\r\n            </ModalBody>\r\n          </Modal>\r\n        ) : (\r\n          \"\"\r\n        )}\r\n      </Container>\r\n    </section>\r\n  );\r\n};\r\n\r\nexport default ProductSection;\r\n"]},"metadata":{},"sourceType":"module"}