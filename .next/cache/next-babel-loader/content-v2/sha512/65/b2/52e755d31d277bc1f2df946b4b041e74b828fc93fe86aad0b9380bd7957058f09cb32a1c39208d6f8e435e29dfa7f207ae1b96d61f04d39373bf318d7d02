}\r\n                onClick={() => setActiveTab(\"furniture\")}\r\n              >\r\n                FEATURED{\" \"}\r\n              </Tab>\r\n              <Tab\r\n                className={activeTab == \"furniture\" ? \"active\" : \"\"}\r\n                onClick={() => setActiveTab(\"furniture\")}\r\n              >\r\n                SPECIAL\r\n              </Tab>\r\n            </TabList>\r\n\r\n            <TabPanel>\r\n              <TabContent\r\n                data={data}\r\n                loading={loading}\r\n                cartClass={cartClass}\r\n                startIndex={0}\r\n                endIndex={8}\r\n              />\r\n            </TabPanel>\r\n            <TabPanel>\r\n              <TabContent\r\n                data={data}\r\n                loading={loading}\r\n                cartClass={cartClass}\r\n                startIndex={0}\r\n                endIndex={8}\r\n              />\r\n            </TabPanel>\r\n            <TabPanel>\r\n              <TabContent\r\n                data={data}\r\n                loading={loading}\r\n                cartClass={cartClass}\r\n                startIndex={0}\r\n                endIndex={8}\r\n              />\r\n            </TabPanel>\r\n          </Tabs>\r\n        </Container>\r\n      </section>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default SpecialProducts;\r\n"]},"metadata":{},"sourceType":"module"}