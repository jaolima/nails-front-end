iqueSize = [];\r\n\r\n  const changeQty = (e) => {\r\n    setQuantity(parseInt(e.target.value));\r\n  };\r\n\r\n  return (\r\n    <>\r\n      <div className={`product-right ${stickyClass}`}>\r\n        Detail Price\r\n        <h2> {product.title} </h2>\r\n        <h4>\r\n          <del>\r\n            {symbol}\r\n            {product.price}\r\n          </del>\r\n          <span>{product.discount}% off</span>\r\n        </h4>\r\n        <h3>\r\n          {symbol}\r\n          {product.price - (product.price * product.discount) / 100}\r\n        </h3>\r\n        {product.variants.map((vari) => {\r\n          var findItem = uniqueColor.find((x) => x.color === vari.color);\r\n          if (!findItem) uniqueColor.push(vari);\r\n          var findItemSize = uniqueSize.find((x) => x === vari.size);\r\n          if (!findItemSize) uniqueSize.push(vari.size);\r\n        })}\r\n        {changeColorVar === undefined ? (\r\n          <>\r\n            {uniqueColor ? (\r\n              <ul className=\"color-variant\">\r\n                {uniqueColor.map((vari, i) => {\r\n                  return (\r\n                    <li className={vari.color} key={i} title={vari.color}></li>\r\n                  );\r\n                })}\r\n              </ul>\r\n            ) : (\r\n              \"\"\r\n            )}\r\n          </>\r\n        ) : (\r\n          <>\r\n            {uniqueColor ? (\r\n              <ul className=\"color-variant\">\r\n                {uniqueColor.map((vari, i) => {\r\n                  return (\r\n                    <li\r\n                      className={vari.color}\r\n                      key={i}\r\n                      title={vari.color}\r\n                      onClick={() => changeColorVar(i)}\r\n                    ></li>\r\n                  );\r\n                })}\r\n              </ul>\r\n            ) : (\r\n              \"\"\r\n            )}\r\n          </>\r\n        )}\r\n        <div className=\"product-description border-product\">\r\n          {product.variants ? (\r\n            <div>\r\n              <h6 className=\"product-title size-text\">\r\n                select size\r\n                <span>\r\n                  <a\r\n                    href={null}\r\n                    data-toggle=\"modal\"\r\n                    data-target=\"#sizemodal\"\r\n                    onClick={toggle}\r\n                  >\r\n                    size chart\r\n                  </a>\r\n                </span>\r\n              </h6>\r\n              <Modal isOpen={modal} toggle={toggle} centered>\r\n                <ModalHeader toggle={toggle}>Sheer Straight Kurta</ModalHeader>\r\n                <ModalBody>\r\n                  <Media src={sizeChart} alt=\"size\" className=\"img-fluid\" />\r\n                </ModalBody>\r\n              </Modal>\r\n              <div className=\"size-box\">\r\n                <ul>\r\n                  {uniqueSize.map((data, i) => {\r\n                    return (\r\n                      <li key={i}>\r\n                        <a href={null}>{data}</a>\r\n                      </li>\r\n                    );\r\n                  })}\r\n                </ul>\r\n              </div>\r\n            </div>\r\n          ) : (\r\n            \"\"\r\n          )}\r\n          <span className=\"instock-cls\">{stock}</span>\r\n          <h6 className=\"product-title\">quantity</h6>\r\n          <div className=\"qty-box\">\r\n            <div className=\"input-group\">\r\n              <span className=\"input-group-prepend\">\r\n                <button\r\n                  type=\"button\"\r\n                  className=\"btn quantity-left-minus\"\r\n                  onClick={minusQty}\r\n                  data-type=\"minus\"\r\n                  data-field=\"\"\r\n                >\r\n                  <i className=\"fa fa-angle-left\"></i>\r\n                </button>\r\n              </span>\r\n              <Input\r\n                type=\"text\"\r\n                name=\"quantity\"\r\n                value={quantity}\r\n                onChange={changeQty}\r\n                className=\"form-control input-number\"\r\n              />\r\n              <span className=\"input-group-prepend\">\r\n                <button\r\n                  type=\"button\"\r\n                  className=\"btn quantity-right-plus\"\r\n                  onClick={() => plusQty(product)}\r\n                  data-type=\"plus\"\r\n                  data-field=\"\"\r\n                >\r\n                  <i className=\"fa fa-angle-right\"></i>\r\n                </button>\r\n              </span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div className=\"product-buttons\">\r\n          <a\r\n            href={null}\r\n            className=\"btn btn-solid\"\r\n            onClick={() => context.addToCart(product, quantity)}\r\n          >\r\n            add to cart\r\n          </a>\r\n          <Link href={`/page/account/checkout`}>\r\n            <a className=\"btn btn-solid\">buy now</a>\r\n          </Link>\r\n        </div>\r\n        <div className=\"border-product\">\r\n          <h6 className=\"product-title\">product details</h6>\r\n          <p>{product.description}</p>\r\n        </div>\r\n        <div className=\"border-product\">\r\n          <h6 className=\"product-title\">share it</h6>\r\n          <div className=\"product-icon\">\r\n            <MasterSocial />\r\n          </div>\r\n        </div>\r\n        <div className=\"border-product\">\r\n          <h6 className=\"product-title\">Time Reminder</h6>\r\n          <CountdownComponent />\r\n        </div>\r\n      </div>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default DetailsWithPrice;\r\n"]},"metadata":{},"sourceType":"module"}